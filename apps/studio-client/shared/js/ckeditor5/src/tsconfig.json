{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "es5",
      "es6",
      "esnext"
    ],
    "module": "commonJS",
    "moduleResolution": "nodenext",
    "newLine": "LF",
    "removeComments": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "composite": true,
    "incremental": true,
    "types": [
      "@ckeditor/ckeditor5-editor-classic"
    ],
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "../dist",
    "declaration": true,
    "declarationMap": true
  }
}
